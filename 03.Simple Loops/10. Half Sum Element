function solve(input){
let n = Number(input.shift());
let max = -99999999999;
let sum = 0;

for(var j = 1;j <= n;j++){
  let num = Number(input.shift());
  sum += num;
  if(num > max){
    max = num;
    }
  }
   let diff = Math.abs(sum - max);
   let diferent = Math.abs(max - (sum - max));
     if(diff == max){
      console.log("Yes");
      console.log("Sum = " + max)
    }
    else{
    console.log("No");
    console.log("Diff = " + diferent)
    }
}
