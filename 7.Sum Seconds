function sumSecond([arg1,arg2,arg3]) {
 let num1 = Number(arg1);
 let num2 = Number(arg2);
 let num3 = Number(arg3);
 let allSecond = num1 + num2 + num3;
 let mins = 0.0;

 
 if(allSecond >= 60){
   mins = mins + 1; //mins++
   allSecond = allSecond - 60;
}
if(allSecond >= 60){
  mins = mins + 1; //mins++
  allSecond = allSecond - 60;
}
 
if(allSecond < 10){
 console.log(mins + ":0" + allSecond)
}
else{
  console.log(mins + ":" + allSecond)
}
}
