using System;

namespace _04_Histogram
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int factorial = 1;

            int i = 1;
            while (i <= n)
            {
                factorial *= i;
                i++;
            }
            Console.WriteLine("{0}! = {1}" , n, factorial);
        }
    }
}
